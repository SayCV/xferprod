{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--metadata-file=data/metadata.toml",
                "--debug",
            ]
        },
        {
            "name": "Test",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "xfer",
                "--metadata-file=data/metadata.toml",
                "--model=archiveflow",
                "--debug",
            ]
        }
    ]
}